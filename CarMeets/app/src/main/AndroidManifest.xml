<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gss130.carmeets">

    <!-- Internet Permissions -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ForgottenPassword" />
        <activity android:name=".ChangePassword" />
        <activity android:name=".Register" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".CreateEventActivity" />
        <activity android:name=".ProfileEditActivity" />
        <activity android:name=".EventsDisplayTypeActivity" />
        <activity android:name=".ViewEventsActivity" />
        <activity android:name=".EventInformationActivity"></activity>
    </application>

</manifest>